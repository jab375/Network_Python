#Int Modules
import os
from ciscoconfparse import CiscoConfParse

#Discover File
for file in os.listdir("/home/jbowers/Configs"):
    if file.startswith("P"):
        configfiles = (os.path.join("/home/jbowers/Configs/", file))
        print configfiles

#Define all Interfaces without Authentication * commands
for item in configfiles:
    parse = CiscoConfParse(item)
    all_intfs = parse.find_objects(r"^interf")
    NoDot1x = list()
    NoDot1x = parse.find_objects_wo_child(r'^interface', r'authentication')

    #Remove Non Gig Ports
    GigPorts = [x.text.split()[1] for x in NoDot1x if x.text.startswith("interface Gig")]

    #Display Filtered Results
    for item in GigPorts: print(item)
      
      
      
      
#List has all wanted files
