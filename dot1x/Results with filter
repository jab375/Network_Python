from ciscoconfparse import CiscoConfParse
parse = CiscoConfParse("/home/jbowers/Configs/PVLSWHINC01")

#Define all Interfaces without Authentication * commands
all_intfs = parse.find_objects(r"^interf")
NoDot1x = list()
