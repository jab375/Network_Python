from ciscoconfparse import CiscoConfParse
parse = CiscoConfParse("/home/jbowers/Configs/PVLSWHINC01")

#Define all Interfaces without Authentication * commands
all_intfs = parse.find_objects(r"^interf")
NoDot1x = list()
NoDot1x = parse.find_objects_wo_child(r'^interface', r'authentication')

#Remove results
for item in NoDot1x: 
 if "interface FastEthernet1": 
  NoDot1x.remove(item) 

#Display Results
for obj in NoDot1x:
    print obj.text




